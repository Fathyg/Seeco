@model IEnumerable<Seeco.Models.TblContracts>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BaseValue)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Period)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Notes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Client)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConsultantContractor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConsultantDesign)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConsultantSupervision)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contractor)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BaseValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Period)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Notes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Client.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ConsultantContractor.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ConsultantDesign.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ConsultantSupervision.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contractor.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
